{
  "name": "tonyu1-wiki",
  "description": "Tonyu System 1 „ÅÆwiki",
  "version": "1.0.0",
  "main": "tool/conv.js",
  "scripts": {
    "debug": "npm run debug:n && npm run debug:spa",
    "debug:n": "tsc tool/conv.ts --sourceMap",
    "debug:spa": "tsc tool/conv-spa.ts --sourceMap",
    "build": "npm run build:n && npm run build:spa",
    "build:n": "ts-node tool/conv",
    "build:spa": "ts-node tool/conv-spa",
    "build:tsc": "npm run build:tsc:n && npm run build:tsc:spa",
    "build:tsc:n": "tsc tool/conv.ts && node tool/conv.js",
    "build:tsc:spa": "tsc tool/conv-spa.ts && node tool/conv-spa.js",
    "fullbuild": "ts-node tool/conv --fullbuild && ts-node tool/conv-spa --fullbuild",
    "deploy": "ts-node tool/conv --deploy && ts-node tool/conv-spa --deploy",
    "server": "http-server -o /docs/",
    "start": "npm run run && npm run server"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makkii-bcr/Tonyu1-wiki.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makkii-bcr/Tonyu1-wiki/issues"
  },
  "homepage": "https://github.com/makkii-bcr/Tonyu1-wiki#readme",
  "devDependencies": {
    "@types/marked": "^2.0.2",
    "@types/node": "^14.14.43",
    "@types/pngjs": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "http-server": "^0.12.3",
    "image-size": "^1.0.0",
    "marked": "^2.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
